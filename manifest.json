{
  "manifest_version": 2,
  "name": "Playback",
  "description": "An extension that lets users add comments to any HTML5 video.",
  "version": "0.1",
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-2.1.3.min.js", "content.js"]
  }
],
"browser_action": {
  "default_icon": "public/icon1.png"
},
"background": {
    "scripts": [
        "background.js"
    ]
}
}
